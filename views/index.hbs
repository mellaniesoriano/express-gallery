<div class='all-photos'>
  {{#each allPhotos}}
  <div class="list-view">
    <a href="/gallery/{{id}}" class="linkButtons"><div class="each-photo"><img src="{{link}}" width="600px"></div></a>
    <p>Author: {{author}}</p>
    <p>Description: {{description}}</p>
    <p>ID: {{id}}</p>
  </div>
  {{/each}}
</div>

<div class="new-link">
  <a href="/gallery/new" class="linkButtons"><div class="addNew">Add New Image</div></a>
</div>